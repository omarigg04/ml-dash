{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist/ml-dash",
    "devCommand": "npm start",
    "installCommand": "npm install",
    "framework": "angular",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/**/*.ts": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}